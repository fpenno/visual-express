{
  "info": [
    {
      "version": "0.0.1",
      "app": "vxpress-lambda",
      "mode": "lambda",
      "logs": "silly"
    }
  ],
  "paths": [
    {
      "cwd": "/Users/felipe/Documents/_personal/projects/AWS/piscestek/github/piscestek/visual-express",
      "appRoot": "/var/local/app",
      "prefix": "",
      "handlers": "/handlers",
      "fsModeFile": 644,
      "fsModeDir": 755
    }
  ],
  "routes": [
    {
      "execOrder": 7,
      "active": true,
      "method": "use",
      "path": null,
